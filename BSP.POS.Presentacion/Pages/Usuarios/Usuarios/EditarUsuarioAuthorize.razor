@page "/configuraciones/usuario/editar/{codigo}"

@attribute [Authorize]
<AuthorizeView>
    <Authorized>
        <EditarUsuario codigo="@codigo" codigoCliente="@cliente"></EditarUsuario>
    </Authorized>
</AuthorizeView>
@code {
    [Parameter]
    public string codigo { get; set; } = string.Empty;
