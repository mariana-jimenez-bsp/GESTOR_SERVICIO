@inject IJSRuntime JSRuntime

@code {
    [Parameter]
    public string IdForm { get; set; } = string.Empty;
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            
            await JSRuntime.InvokeVoidAsync("desactivarEnterSubmit", IdForm);
        }

    }
    
}
